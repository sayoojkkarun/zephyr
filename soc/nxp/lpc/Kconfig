config SOC_FAMILY_LPC
	select HAS_SEGGER_RTT if ZEPHYR_SEGGER_MODULE
	select CLOCK_CONTROL
	select ARM

config SOC_PART_NUMBER
	default SOC_PART_NUMBER_LPC84X if SOC_SERIES_LPC84X

if SOC_FAMILY_LPC
rsource "*/Kconfig"

endif # SOC_FAMILY_LPC
